# Configuration Coverage.py pour E-CMS
# https://coverage.readthedocs.io/en/latest/config.html

[run]
source = .
omit = 
    */migrations/*
    */tests.py
    */__init__.py
    manage.py
    */management/commands/*
    ecms_config/asgi.py
    ecms_config/wsgi.py
    */views.py

branch = True
parallel = True

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract

show_missing = True
precision = 2

[html]
directory = htmlcov
title = E-CMS Coverage Report

[xml]
output = coverage.xml
